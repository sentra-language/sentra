let arr = [64, 34, 25]
log("Initial: " + arr)

// Test array access
let i = 1
log("i = " + i)
log("arr[i] = " + arr[i])
log("arr[i+1] = " + arr[i+1])

// Now swap
let temp = arr[i]
log("temp = " + temp)
arr[i] = arr[i+1]
log("After arr[i] = arr[i+1]: " + arr)
log("Now arr[i] = " + arr[i])
arr[i+1] = temp  
log("After arr[i+1] = temp: " + arr)