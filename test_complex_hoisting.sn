// Complex hoisting test matching cloud_security_demo pattern

fn func_a() { return "A" }
fn func_b() { return "B" }
fn func_c() { return "C" }
fn func_d() { return "D" }
fn func_e() { return "E" }

fn wrapper1() {
    return func_c()  // Call a function defined earlier
}

fn func_f() { return "F" }
fn func_g() { return "G" }

fn wrapper2() {
    return func_g()  // Call a function defined later
}

fn func_h() { return "H" }

// Now call them
log("Direct calls:")
log(func_a())
log(func_h())

log("Wrapped calls:")
log(wrapper1())
log(wrapper2())