let arr = [64, 34, 25, 12]
log("Initial: " + arr)

let pass = 0
while pass < 2 {
    log("\nPass " + pass + ":")
    let i = 0
    let limit = len(arr) - pass - 1
    log("  Will check " + limit + " pairs")
    
    while i < limit {
        log("    i=" + i)
        log("      arr[" + i + "] = " + arr[i])
        log("      arr[" + (i+1) + "] = " + arr[i + 1])
        
        if arr[i] > arr[i + 1] {
            let a = arr[i]
            let b = arr[i + 1]
            log("      Swapping: a=" + a + ", b=" + b)
            arr[i] = b
            arr[i + 1] = a
            log("      After: " + arr)
        }
        i = i + 1
    }
    pass = pass + 1
}

log("\nFinal: " + arr)