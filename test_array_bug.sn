let arr = [34, 64, 25]
log("Array: " + arr)
log("arr[0] = " + arr[0])
log("arr[1] = " + arr[1])
log("arr[2] = " + arr[2])

let i = 1
log("\nUsing variable i=1:")
log("arr[i] = " + arr[i])
log("arr[i+1] = " + arr[i+1])

log("\nDirect access:")
log("arr[1] = " + arr[1])
log("arr[2] = " + arr[2])

log("\nStoring in variables:")
let val_at_1 = arr[1]
let val_at_2 = arr[2]
log("val_at_1 = " + val_at_1)
log("val_at_2 = " + val_at_2)

log("\nStoring with i:")
let val_at_i = arr[i]
let val_at_i_plus_1 = arr[i + 1]
log("val_at_i = " + val_at_i)
log("val_at_i_plus_1 = " + val_at_i_plus_1)