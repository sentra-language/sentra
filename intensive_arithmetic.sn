// Intensive arithmetic test to measure type conversion overhead
log("Starting intensive arithmetic tests...")

// Test 1: Pure integer arithmetic
let sum1 = 0
for (let i = 0; i < 50000; i = i + 1) {
    sum1 = sum1 + i
    sum1 = sum1 - 1
    sum1 = sum1 + 2
}
log("Integer test complete: " + sum1)

// Test 2: Mixed arithmetic (triggers type conversion)
let sum2 = 0.0
for (let i = 0; i < 50000; i = i + 1) {
    sum2 = sum2 + i * 2.0 - 1.0
    sum2 = sum2 / 1.1
    sum2 = sum2 * 0.9
}
log("Mixed arithmetic complete: " + sum2)

// Test 3: Heavy string operations
let text = "start"
for (let i = 0; i < 2000; i = i + 1) {
    text = text + " test " + i
    if len(text) > 10000 {
        text = "reset"
    }
}
log("String test complete: " + len(text))

log("All tests finished")