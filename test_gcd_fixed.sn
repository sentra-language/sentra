fn gcd(a, b) {
    log("Start: a=" + a + ", b=" + b)
    let count = 0
    while b != 0 && count < 5 {
        log("Loop " + count + ": a=" + a + ", b=" + b)
        let temp = b
        log("  temp = " + temp)
        let newB = a % b
        log("  newB (a % b) = " + newB)
        b = newB
        a = temp
        log("  After assignment: a=" + a + ", b=" + b)
        count = count + 1
    }
    return a
}

log("GCD(48, 18) = " + gcd(48, 18))