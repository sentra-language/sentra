let arr = [64, 34, 25]
log("Initial: " + arr)

// First iteration
let i = 0
log("\nIteration " + i + ":")
if arr[i] > arr[i + 1] {
    let a = arr[i]
    let b = arr[i + 1]
    log("  Swapping " + a + " and " + b)
    arr[i] = b
    arr[i + 1] = a
}
log("  After: " + arr)

// Second iteration
i = 1
log("\nIteration " + i + ":")
log("  arr[" + i + "] = " + arr[i])
log("  arr[" + (i+1) + "] = " + arr[i + 1])

if arr[i] > arr[i + 1] {
    let a = arr[i]
    let b = arr[i + 1]
    log("  Values to swap: a=" + a + ", b=" + b)
    arr[i] = b
    arr[i + 1] = a
}
log("  After: " + arr)