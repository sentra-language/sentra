fn bubbleSort(arr) {
    let n = len(arr)
    let i = 0
    while i < n - 1 {
        let j = 0
        while j < n - i - 1 {
            if arr[j] > arr[j + 1] {
                let temp = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = temp
            }
            j = j + 1
        }
        i = i + 1
    }
    return arr
}

let test = [64, 34, 25, 12, 22, 11, 90]
log("Original: " + test)
let sorted = bubbleSort(test)
log("Sorted: " + sorted)