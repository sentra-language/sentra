#!/usr/bin/env sentra
// Simple test for build system

import "./src/simple_scanner.sn"
import "./src/simple_utils.sn"

fn main() {
    log("=== Simple Build Test ===")
    
    // Test utils
    let title = FormatTitle("Testing")
    log(title)
    
    // Test scanner
    let ports = ScanRange("localhost", 80, 82)
    log("Found ports:")
    for p in ports {
        log("  - " + str(p))
    }
    
    // Test math
    let bigger = Max(10, 20)
    log("Max value: " + str(bigger))
    
    log("Build test complete!")
}

main()