#!/usr/bin/env sentra

log("Testing array manipulation functions:")

// Create test array
let arr = [1, 2, 3, 4, 5]
log(arr)

// Test push (already exists)
push(arr, 6)
log(arr)

// Test shift - remove first element
let first = shift(arr)
log(first)
log(arr)

// Test unshift - add element at beginning
unshift(arr, 0)
log(arr)

// Test slice - create sub-array
let sub = slice(arr, 1, 4)
log(sub)

// Test remove - remove element at index
let removed = remove(arr, 2)
log(removed)
log(arr)

// Test insert - insert element at index
insert(arr, 2, "inserted")
log(arr)

// Test array_contains
let hasThree = array_contains(arr, 3)
log(hasThree)
let hasTen = array_contains(arr, 10)
log(hasTen)

// Test index_of
let idx = index_of(arr, 4)
log(idx)
let notFound = index_of(arr, 99)
log(notFound)

// Test join
let str = join(arr, ", ")
log(str)

// Test array_sort
let unsorted = [5, 2, 8, 1, 9, 3]
log(unsorted)
let sorted = array_sort(unsorted)
log(sorted)
log(unsorted)

// Test clear
let temp = [1, 2, 3]
log(temp)
clear(temp)
log(temp)

log("All array function tests completed!")