{
  "build": {
    "env": {
      "JEKYLL_ENV": "production"
    }
  },
  "builds": [
    {
      "src": "docs/**",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd docs && bundle install && bundle exec jekyll build",
        "outputDirectory": "docs/_site"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/docs/_site/$1"
    }
  ]
}