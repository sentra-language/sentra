// Test simple sorting
fn bubbleSort(arr) {
    let n = len(arr)
    log("Array length: " + n)
    
    for i in range(0, n - 1) {
        for j in range(0, n - i - 1) {
            if arr[j] > arr[j + 1] {
                let temp = arr[j]
                arr[j] = arr[j + 1]
                arr[j + 1] = temp
            }
        }
    }
    return arr
}

let test = [5, 3, 1, 4, 2]
log("Original: " + test)
log("Sorted: " + bubbleSort(test))