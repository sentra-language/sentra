log("Testing mem_get_process_info...")

let explorer_procs = mem_find_process("explorer.exe")
if (len(explorer_procs) > 0) {
    let proc = explorer_procs[0]
    log("Found explorer.exe: PID " + str(proc["pid"]))
    
    log("About to call mem_get_process_info...")
    let detailed = mem_get_process_info(proc["pid"])
    log("Got detailed info, checking if exists...")
    
    if (detailed) {
        log("Has detailed info")
        log("Type: " + type(detailed))
        if (detailed["pid"]) {
            log("PID: " + str(detailed["pid"]))
        }
        if (detailed["name"]) {
            log("Name: " + detailed["name"])
        }
    } else {
        log("No detailed info")
    }
} else {
    log("Explorer.exe not found")
}

log("Test completed!")