fn gcd(a, b) {
    log("Computing GCD(" + a + ", " + b + ")")
    while b != 0 {
        log("  a=" + a + ", b=" + b)
        let temp = b
        b = a % b
        a = temp
        log("  After: a=" + a + ", b=" + b)
    }
    log("  Result: " + a)
    return a
}

log("GCD(48, 18) = " + gcd(48, 18))
log("GCD(12, 8) = " + gcd(12, 8))