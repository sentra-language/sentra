let arr = [64, 34, 25]
log("Initial: " + arr)

let i = 0
while i < 2 {
    log("\nIteration " + i + ":")
    log("  arr[" + i + "] = " + arr[i])
    log("  arr[" + (i+1) + "] = " + arr[i + 1])
    
    if arr[i] > arr[i + 1] {
        let a = arr[i]
        let b = arr[i + 1]
        log("  Values to swap: a=" + a + ", b=" + b)
        arr[i] = b
        arr[i + 1] = a
        log("  After swap: " + arr)
    }
    i = i + 1
}

log("\nFinal: " + arr)