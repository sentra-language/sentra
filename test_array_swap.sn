let arr = [10, 20, 30]
log("Initial: " + arr)
log("arr[0] = " + arr[0])
log("arr[1] = " + arr[1])
log("arr[2] = " + arr[2])

// Swap 0 and 1
let temp = arr[0]
arr[0] = arr[1]
arr[1] = temp
log("After swap 0,1: " + arr)
log("arr[0] = " + arr[0])
log("arr[1] = " + arr[1])
log("arr[2] = " + arr[2])

// Now swap 1 and 2
temp = arr[1]
arr[1] = arr[2]
arr[2] = temp
log("After swap 1,2: " + arr)
log("arr[0] = " + arr[0])
log("arr[1] = " + arr[1])
log("arr[2] = " + arr[2])