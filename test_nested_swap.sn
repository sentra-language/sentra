let arr = [64, 34, 25]
log("Initial: " + arr)

let pass = 0
while pass < 1 {
    log("Pass " + pass)
    let i = 0
    while i < 2 {
        log("  i=" + i + ", arr[i]=" + arr[i] + ", arr[i+1]=" + arr[i+1])
        if arr[i] > arr[i+1] {
            let temp = arr[i]
            let next = arr[i+1]
            log("    Swapping " + temp + " and " + next)
            arr[i] = next
            arr[i+1] = temp
            log("    After swap: " + arr)
        }
        i = i + 1
    }
    pass = pass + 1
}

log("Final: " + arr)