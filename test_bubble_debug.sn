fn bubbleSort(arr) {
    // Create a copy to avoid modifying original  
    let result = []
    for item in arr {
        result.push(item)
    }
    log("Copy: " + result)
    
    let n = len(result)
    let i = 0
    while i < n - 1 {
        log("Pass " + i)
        let j = 0
        while j < n - i - 1 {
            log("  Comparing " + result[j] + " and " + result[j + 1])
            if result[j] > result[j + 1] {
                // Swap elements
                log("    Swapping!")
                let temp = result[j]
                result[j] = result[j + 1]
                result[j + 1] = temp
            }
            j = j + 1
        }
        log("  After pass: " + result)
        i = i + 1
    }
    return result
}

let test = [64, 34, 25]
log("Original: " + test)
let sorted = bubbleSort(test)
log("Sorted: " + sorted)